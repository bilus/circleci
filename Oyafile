# -*- mode: yaml -*-

Project: example

Changeset: |
  changed_dirs=$(git diff-tree master..HEAD | awk '$1$2~/04/' | cut -f2-;)
  if [ -z "${changed_dirs}" ]; then
    exit 0
  fi

  echo "+/"
  echo "${changed_dirs}" | awk '{a="+/"$1; print a}'

onPush: |
  echo "Pushed to branch"

onMerge: |
  echo "Building master"


# onMergeSuccess: |
